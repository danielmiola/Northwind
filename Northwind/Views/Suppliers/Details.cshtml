@model Northwind.Models.Suppliers

@{
    ViewBag.Title = "Details";
}

<dl class="dl-horizontal">
    
    <dt>Company name</dt>
    <dd>@Html.DisplayFor(model => model.CompanyName)</dd>

    <dt>Contact</dt>
    <dd>@Html.DisplayFor(model => model.ContactName)</dd>

    <dt>Contact tittle</dt>
    <dd>@Html.DisplayFor(model => model.ContactTitle)</dd>

    <dt>Address</dt>
    <dd>@Html.DisplayFor(model => model.Address)</dd>

    <dt>City</dt>
    <dd>@Html.DisplayFor(model => model.City)</dd>

    <dt>Regions</dt>
    <dd>@Html.DisplayFor(model => model.Region)</dd>

    <dt>Postal code</dt>
    <dd>@Html.DisplayFor(model => model.PostalCode)</dd>

    <dt>Country</dt>
    <dd>@Html.DisplayFor(model => model.Country)</dd>

    <dt>Phone</dt>
    <dd>@Html.DisplayFor(model => model.Phone)</dd>

    <dt>Fax</dt>
    <dd>@Html.DisplayFor(model => model.Fax)</dd>

    <dt>Home page</dt>
    <dd>@Html.DisplayFor(model => model.HomePage)</dd>

</dl>
